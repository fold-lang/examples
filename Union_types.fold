-- Untion types

-- Canonical. OCaml.
type Visibility =
    | All
    | Active
    | Completed

to_string :: Visibility -> String
to_string visibility =
    case visibility
        All -> "All"
        Active -> "Active"
        Completed -> "Completed"
    end

to_string visibility =
    case visibility
    | All -> "All"
    | Active -> "Active"
    | Completed -> "Completed"
    end

to_string visibility =
    case visibility
      | All -> "All"
      | Active -> "Active"
      | Completed -> "Completed"
    end

